{
    "name": "vscode-qt-for-cpp",
    "version": "0.4.2",
    "publisher": "seanwu",
    "engines": {
        "vscode": "^1.30.0"
    },
    "license": "MIT",
    "displayName": "Qt for C++",
    "description": "Qt for C++ (PyQt5 and PySide2) extension for Visual Studio Code.",
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "keywords": [
        "Qt",
        "QML",
        "PyQt",
        "PySide",
        "QtQuick"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "qtForCPP.newForm",
                "title": "New Form (Qt Designer UI File)",
                "category": "Qt for C++"
            },
            {
                "command": "qtForCPP.editForm",
                "title": "Edit Form (Qt Designer UI File)",
                "category": "Qt for C++"
            },
            {
                "command": "qtForCPP.compileForm",
                "title": "Compile Form (Qt Designer UI File) into Qt for C++ File",
                "category": "Qt for C++"
            },
            {
                "command": "qtForCPP.updateTranslation",
                "title": "Update Qt Linguist Translation File from Qt for C++",
                "category": "Qt for C++"
            },
            {
                "command": "qtForCPP.editTranslation",
                "title": "Edit Qt Linguist Translation File",
                "category": "Qt for C++"
            },
            {
                "command": "qtForCPP.releaseTranslation",
                "title": "Release Qt Linguist Translation to QM File",
                "category": "Qt for C++"
            },
            {
                "command": "qtForCPP.previewQml",
                "title": "Preview QML File",
                "category": "Qt for C++"
            },
            {
                "command": "qtForCPP.compileResource",
                "title": "Compile Resource File into Qt for C++ File",
                "category": "Qt for C++"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "qtForCPP.editForm",
                    "when": "resourceExtname == .ui && resourceLangId == xml"
                },
                {
                    "command": "qtForCPP.compileForm",
                    "when": "resourceExtname == .ui && resourceLangId == xml"
                },
                {
                    "command": "qtForCPP.updateTranslation",
                    "when": "resourceLangId == c++"
                },
                {
                    "command": "qtForCPP.editTranslation",
                    "when": "resourceExtname == .ts  && resourceLangId == xml"
                },
                {
                    "command": "qtForCPP.releaseTranslation",
                    "when": "resourceExtname == .ts  && resourceLangId == xml"
                },
                {
                    "command": "qtForCPP.previewQml",
                    "when": "resourceLangId == qml"
                },
                {
                    "command": "qtForCPP.compileResource",
                    "when": "resourceExtname == .qrc && resourceLangId == xml"
                }
            ],
            "explorer/context": [
                {
                    "command": "qtForCPP.newForm",
                    "when": "explorerResourceIsFolder == true",
                    "group": "qtForCPP@1"
                },
                {
                    "command": "qtForCPP.editForm",
                    "when": "resourceExtname == .ui && resourceLangId == xml",
                    "group": "qtForCPP@2"
                },
                {
                    "command": "qtForCPP.compileForm",
                    "when": "resourceExtname == .ui && resourceLangId == xml",
                    "group": "qtForCPP@3"
                },
                {
                    "command": "qtForCPP.updateTranslation",
                    "when": "resourceLangId == c++",
                    "group": "qtForCPP@4"
                },
                {
                    "command": "qtForCPP.editTranslation",
                    "when": "resourceExtname == .ts && resourceLangId == xml",
                    "group": "qtForCPP@5"
                },
                {
                    "command": "qtForCPP.releaseTranslation",
                    "when": "resourceExtname == .ts && resourceLangId == xml",
                    "group": "qtForCPP@6"
                },
                {
                    "command": "qtForCPP.previewQml",
                    "when": "resourceLangId == qml",
                    "group": "qtForCPP@7"
                },
                {
                    "command": "qtForCPP.compileResource",
                    "when": "resourceExtname == .qrc && resourceLangId == xml",
                    "group": "qtForCPP@8"
                }
            ],
            "editor/title": [
                {
                    "command": "qtForCPP.editForm",
                    "when": "resourceExtname == .ui && resourceLangId == xml",
                    "group": "qtForCPP@2"
                },
                {
                    "command": "qtForCPP.compileForm",
                    "when": "resourceExtname == .ui && resourceLangId == xml",
                    "group": "qtForCPP@3"
                },
                {
                    "command": "qtForCPP.updateTranslation",
                    "when": "resourceLangId == c++",
                    "group": "qtForCPP@4"
                },
                {
                    "command": "qtForCPP.editTranslation",
                    "when": "resourceExtname == .ts && resourceLangId == xml",
                    "group": "qtForCPP@5"
                },
                {
                    "command": "qtForCPP.releaseTranslation",
                    "when": "resourceExtname == .ts && resourceLangId == xml",
                    "group": "qtForCPP@6"
                },
                {
                    "command": "qtForCPP.previewQml",
                    "when": "resourceLangId == qml",
                    "group": "qtForCPP@7"
                },
                {
                    "command": "qtForCPP.compileResource",
                    "when": "resourceExtname == .qrc && resourceLangId == xml",
                    "group": "qtForCPP@8"
                }
            ],
            "editor/context": [
                {
                    "command": "qtForCPP.editForm",
                    "when": "resourceExtname == .ui && resourceLangId == xml",
                    "group": "qtForCPP@2"
                },
                {
                    "command": "qtForCPP.compileForm",
                    "when": "resourceExtname == .ui && resourceLangId == xml",
                    "group": "qtForCPP@3"
                },
                {
                    "command": "qtForCPP.updateTranslation",
                    "when": "resourceLangId == c++",
                    "group": "qtForCPP@4"
                },
                {
                    "command": "qtForCPP.editTranslation",
                    "when": "resourceExtname == .ts && resourceLangId == xml",
                    "group": "qtForCPP@5"
                },
                {
                    "command": "qtForCPP.releaseTranslation",
                    "when": "resourceExtname == .ts && resourceLangId == xml",
                    "group": "qtForCPP@6"
                },
                {
                    "command": "qtForCPP.previewQml",
                    "when": "resourceLangId == qml",
                    "group": "qtForCPP@7"
                },
                {
                    "command": "qtForCPP.compileResource",
                    "when": "resourceExtname == .qrc && resourceLangId == xml",
                    "group": "qtForCPP@8"
                }
            ]
        },
        "configuration": {
            "title": "Qt for C++",
            "properties": {
                "qtForCPP.path.designer": {
                    "type": "string",
                    "default": "",
                    "markdownDescription": "The path of Qt Designer with CLI arguments to create and edit form (`*.ui`) files. For example, `\"/opt/Qt/5.13.0/gcc_64/bin/designer\"` in Linux or `\"C:\\Qt\\5.13.0\\mingw73_64\\bin\\designer.exe\"` in Windows."
                },
                "qtForCPP.path.pyuic": {
                    "type": "string",
                    "default": "",
                    "markdownDescription": "The path of UI compiler of Qt for C++ (PyQt5 or PySide2). For example, `pyuic5 -d -o ./\"${fileBasenameNoExtension}.py\"` for PyQt5 or `pyside2-uic -d -o ./\"${fileBasenameNoExtension}.py\"` for PySide2."
                },
                "qtForCPP.path.pylupdate": {
                    "type": "string",
                    "default": "",
                    "markdownDescription": "The path of `lupdate` of Qt for C++ (PyQt5 or PySide2). For example, `pylupdate5 -verbose -ts ./\"${fileBasenameNoExtension}.qt.ts\"` for PyQt5 or `pyside2-lupdate -ts ./\"${fileBasenameNoExtension}.qt.ts\"` for PySide2."
                },
                "qtForCPP.path.linguist": {
                    "type": "string",
                    "default": "",
                    "markdownDescription": "The path of Qt Linguist with CLI arguments to edit translation (`*.qt.ts`) files. For example, `\"/opt/Qt/5.13.0/gcc_64/bin/linguist\"` in Linux or `\"C:\\Qt\\5.13.0\\mingw73_64\\bin\\linguist.exe\"` in Windows."
                },
                "qtForCPP.path.lrelease": {
                    "type": "string",
                    "default": "",
                    "markdownDescription": "The path of Qt lrelease with CLI arguments to edit translation (`*.qt.ts`) files. For example, `\"/opt/Qt/5.13.0/gcc_64/bin/lrelease\" -qm ./out.qm` in Linux or `\"C:\\Qt\\5.13.0\\mingw73_64\\bin\\lrelease.exe\" -qm .\\out.qm` in Windows."
                },
                "qtForCPP.path.qmlscene": {
                    "type": "string",
                    "default": "",
                    "markdownDescription": "The path of Qt QML Scene (`qmlscene`) with CLI arguments to preview QML (`*.qml`) files. For example, `\"/opt/Qt/5.13.0/gcc_64/bin/qmlscene\" --verbose` in Linux or `\"C:\\Qt\\5.13.0\\mingw73_64\\bin\\qmlscene.exe\" --verbose` in Windows."
                },
                "qtForCPP.path.pyrcc": {
                    "type": "string",
                    "default": "",
                    "markdownDescription": "The path of resource file compiler of Qt for C++ (PyQt5 or PySide2). For example, ` pyrcc5 -o ./\"${fileBasenameNoExtension}.py\"` for PyQt5 or `pyside2-rcc -o ./\"${fileBasenameNoExtension}.py\"` for PySide2."
                }
            }
        },
        "languages": [
            {
                "id": "qmake",
                "extensions": [
                    ".pro",
                    ".pri",
                    ".prf",
                    ".prl"
                ],
                "aliases": [
                    "QMake",
                    "qmake",
                    "Qt Project"
                ],
                "configuration": "./languages/qmake-language-configuration.json"
            },
            {
                "id": "qml",
                "extensions": [
                    ".qml"
                ],
                "aliases": [
                    "QML",
                    "qml"
                ],
                "configuration": "./languages/qml-language-configuration.json"
            },
            {
                "id": "qmldir",
                "filenames": [
                    "qmldir"
                ],
                "aliases": [
                    "QML Directory",
                    "qmldir"
                ],
                "configuration": "./languages/qmldir-language-configuration.json"
            },
            {
                "id": "qss",
                "extensions": [
                    ".qss"
                ],
                "aliases": [
                    "QSS",
                    "qss"
                ],
                "configuration": "./languages/qss-language-configuration.json"
            },
            {
                "id": "xml",
                "extensions": [
                    ".qt.ts",
                    ".ui",
                    ".pro.user",
                    ".qrc"
                ]
            }
        ],
        "grammars": [
            {
                "language": "qmake",
                "scopeName": "source.qmake",
                "path": "./syntaxes/qmake.tmLanguage.json"
            },
            {
                "language": "qml",
                "scopeName": "source.qml",
                "embeddedLanguages": {
                    "meta.embedded.block.js": "javascript"
                },
                "path": "./syntaxes/qml.tmLanguage.json"
            },
            {
                "language": "qmldir",
                "scopeName": "source.qmldir",
                "path": "./syntaxes/qmldir.tmLanguage.json"
            },
            {
                "language": "qss",
                "scopeName": "source.qss",
                "path": "./syntaxes/qss.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "qml",
                "path": "./snippets/qml.json"
            },
            {
                "language": "qmldir",
                "path": "./snippets/qmldir.json"
            },
            {
                "language": "qss",
                "path": "./snippets/qss.json"
            }
        ]
    },
    "activationEvents": [
        "onCommand:qtForCPP.newForm",
        "onCommand:qtForCPP.editForm",
        "onCommand:qtForCPP.compileForm",
        "onCommand:qtForCPP.updateTranslation",
        "onCommand:qtForCPP.editTranslation",
        "onCommand:qtForCPP.releaseTranslation",
        "onCommand:qtForCPP.previewQml",
        "onCommand:qtForCPP.compileResource"
    ],
    "badges": [
        {
            "url": "https://img.shields.io/visual-studio-marketplace/v/seanwu.vscode-qt-for-python.svg",
            "href": "https://marketplace.visualstudio.com/items?itemName=seanwu.vscode-qt-for-python",
            "description": "version"
        },
        {
            "url": "https://img.shields.io/visual-studio-marketplace/r/seanwu.vscode-qt-for-python.svg",
            "href": "https://marketplace.visualstudio.com/items?itemName=seanwu.vscode-qt-for-python",
            "description": "version"
        },
        {
            "url": "https://img.shields.io/visual-studio-marketplace/d/seanwu.vscode-qt-for-python.svg",
            "href": "https://marketplace.visualstudio.com/items?itemName=seanwu.vscode-qt-for-python",
            "description": "version"
        },
        {
            "url": "https://img.shields.io/visual-studio-marketplace/i/seanwu.vscode-qt-for-python.svg",
            "href": "https://marketplace.visualstudio.com/items?itemName=seanwu.vscode-qt-for-python",
            "description": "version"
        }
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.4.5",
        "vscode": "^1.1.33",
        "tslint": "^5.16.0",
        "@types/node": "^11.13.8",
        "@types/mocha": "^5.2.6"
    },
    "icon": "images/icon.png",
    "homepage": "https://github.com/daravi/vscode-qt-for-cpp",
    "repository": {
        "type": "git",
        "url": "https://github.com/daravi/vscode-qt-for-cpp.git"
    },
    "bugs": {
        "url": "https://github.com/daravi/vscode-qt-for-cpp/issues",
        "email": "puya.daravi@gmail.com"
    }
}